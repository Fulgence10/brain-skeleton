{
    "name": "brainphp/app",
    "description": "the brain php framework",
    "keywords": ["framework", "brain", "php", "php-framework"],
    "type": "project",
    "license": "MIT",
    "autoload": {
        "files": [
            "./Brain/src/Supports/functions.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Brain\\": "Brain/src/"
        }
    },
    "require": {
        "php": ">=7.0",
        "guzzlehttp/psr7": "^1.5",
        "php-di/php-di": "^6.0",
        "twig/twig": "^2.0"
    },
    "authors": [
        {
            "name": "Fulgence Yapi",
            "email": "yfulgence10@gmail.com",
            "role": "Lead Developer"
        }
    ],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
